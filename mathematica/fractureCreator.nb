Cell[BoxData[
 RowBox[{
  RowBox[{"GenerateFracture", "[", 
   RowBox[{
   "pointA_", ",", "pointB_", ",", "pointC_", ",", "ncorners_", ",", 
    "centroid_", ",", "radius_"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"planeEquation", ",", "normal", ",", "planePolygon", ",", 
      RowBox[{"k", "=", "ncorners"}], ",", "polygon", ",", 
      RowBox[{"fpointA", "=", "pointA"}], ",", 
      RowBox[{"fpointB", "=", "pointB"}], ",", 
      RowBox[{"fpointC", "=", "pointC"}], ",", 
      RowBox[{"fcentroid", "=", "centroid"}], ",", "normalpoly", ",", 
      "fracture"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"planeEquation", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Normalize", "[", 
         RowBox[{"Cross", "[", 
          RowBox[{
           RowBox[{"fpointA", "-", "fpointC"}], ",", 
           RowBox[{"fpointB", "-", "fpointC"}]}], "]"}], "]"}], ".", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"x", ",", "y", ",", "z"}], "}"}], "-", 
          FractionBox[
           RowBox[{"fpointA", "+", "fpointB"}], "2"]}], ")"}]}], "//", 
       "Simplify"}]}], ";", "\[IndentingNewLine]", "\t", 
     RowBox[{
      RowBox[{"Print", "[", 
       RowBox[{"\"\<Plane equation: \\n\\t \>\"", ",", 
        RowBox[{"NumberForm", "[", 
         RowBox[{"planeEquation", ",", "3"}], "]"}], ",", "\"\< = 0\>\""}], 
       "]"}], "//", "Quiet"}], ";", "\[IndentingNewLine]", 
     RowBox[{"normal", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Coefficient", "[", 
         RowBox[{"planeEquation", ",", "x"}], "]"}], ",", 
        RowBox[{"Coefficient", "[", 
         RowBox[{"planeEquation", ",", "y"}], "]"}], ",", 
        RowBox[{"Coefficient", "[", 
         RowBox[{"planeEquation", ",", "z"}], "]"}]}], "}"}]}], ";", 
     "\[IndentingNewLine]", "\t", 
     RowBox[{"Print", "[", 
      RowBox[{
      "\"\<Normal vector: \\n\\t \!\(\*StyleBox[\"n\",FontWeight->\"Bold\"]\) \
= \>\"", ",", 
       RowBox[{"NumberForm", "[", 
        RowBox[{"normal", ",", "3"}], "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"planePolygon", " ", "=", 
      RowBox[{
       RowBox[{"CirclePoints", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"0.", ",", "0."}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"radius", ",", 
           RowBox[{"45", 
            FractionBox["\[Pi]", "180"]}]}], "}"}], ",", "k"}], "]"}], "//", 
       "Transpose"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"polygon", "=", 
      RowBox[{"Rotate", "[", 
       RowBox[{
        RowBox[{"Polygon", "[", 
         RowBox[{"Transpose", "[", 
          RowBox[{"AppendTo", "[", 
           RowBox[{"planePolygon", ",", 
            RowBox[{"ConstantArray", "[", 
             RowBox[{"0.", ",", "k"}], "]"}]}], "]"}], "]"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"0", ",", "0", ",", "1"}], "}"}], ",", "normal"}], "}"}]}],
        "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"normalpoly", "=", 
      RowBox[{"Normal", "[", "polygon", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"fracture", "=", 
      RowBox[{"normalpoly", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", "k"}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", "\t", 
       RowBox[{
        RowBox[{
         RowBox[{"fracture", "[", 
          RowBox[{"[", "i", "]"}], "]"}], "+=", "centroid"}], ";"}]}], 
      "\[IndentingNewLine]", "    ", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{
      "\"\<Corners coordinates: \\n\\t\>\"", ",", "\[IndentingNewLine]", 
       RowBox[{"DecimalForm", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"fracture", "//", "Transpose"}], "//", "MatrixForm"}], ",", 
         "12"}], "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"Graphics3D", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"RGBColor", "[", 
           RowBox[{"0.386", ",", ".665", ",", "1"}], "]"}], ",", 
          RowBox[{"Opacity", "[", ".8", "]"}], ",", "polygon"}], "}"}], ",", 
        RowBox[{"Axes", "\[Rule]", "True"}]}], "]"}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{"fracture", "//", "Transpose"}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
